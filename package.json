{
  "name": "pillars_api",
  "version": "0.1.0",
  "bin": {
    "pillars_api": "bin/pillars_api.js"
  },
  "scripts": {
    "build": "cross-env SUBDOMAIN=local node config/copy-config.js && tsc && browserify web/play.ts -p [ tsify --noImplicitAny ] > web/dist/play-bundle.js && node web/copy-dist.js",
    "build-ezbeard": "cross-env SUBDOMAIN=ezbeard node config/copy-config.js && tsc && browserify web/play.ts -p [ tsify --noImplicitAny ] > web/dist/play-bundle.js && node web/copy-dist.js",
    "watch": "tsc -w",
    "test": "jest --group=unit",
    "integration-test": "jest --group=integration",
    "cdk": "cdk",
    "clean": "node web/clean-dist.js",
    "synth-ezbeard": "cdk synth -c subdomain=ezbeard",
    "deploy-ezbeard": "cdk deploy -c subdomain=ezbeard"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.28.0",
    "@aws-cdk/aws-s3-deployment": "^1.28.0",
    "@types/aws-lambda": "^8.10.46",
    "@types/aws-sdk": "^2.7.0",
    "@types/howler": "^2.1.2",
    "@types/jest": "^25.1.4",
    "@types/jquery": "^3.3.33",
    "@types/node": "^13.9.1",
    "@types/query-string": "^6.3.0",
    "@types/uuid": "^7.0.2",
    "aws-cdk": "^1.28.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "howler": "^2.1.3",
    "jest": "^25.1.0",
    "jest-runner-groups": "^1.1.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3",
    "web-audio-daw": "^4.7.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.28.0",
    "@aws-cdk/aws-dynamodb": "^1.28.0",
    "@aws-cdk/aws-lambda": "^1.28.0",
    "@aws-cdk/core": "^1.28.0",
    "@types/fs-extra": "^8.1.0",
    "aws-sdk": "^2.641.0",
    "fs-extra": "^8.1.0",
    "query-string": "^6.11.1",
    "source-map-support": "^0.5.16",
    "uuid": "^7.0.2"
  }
}
